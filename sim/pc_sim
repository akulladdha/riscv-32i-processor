#! /opt/local/bin/vvp
:ivl_version "13.0 (devel)" "(s20250103)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/opt/local/lib/ivl/system.vpi";
:vpi_module "/opt/local/lib/ivl/vhdl_sys.vpi";
:vpi_module "/opt/local/lib/ivl/vhdl_textio.vpi";
:vpi_module "/opt/local/lib/ivl/v2005_math.vpi";
:vpi_module "/opt/local/lib/ivl/va_math.vpi";
S_0x13f704160 .scope module, "pc_tb" "pc_tb" 2 3;
 .timescale -9 -12;
v0x13f7148a0_0 .var "clk", 0 0;
v0x13f714940_0 .var "pc_in", 31 0;
v0x13f7149f0_0 .net "pc_out", 31 0, v0x13f7146e0_0;  1 drivers
v0x13f714ac0_0 .var "reset", 0 0;
S_0x13f7042d0 .scope module, "uut" "pc" 2 9, 3 1 0, S_0x13f704160;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /INPUT 32 "pc_in";
    .port_info 3 /OUTPUT 32 "pc_out";
v0x13f704580_0 .net "clk", 0 0, v0x13f7148a0_0;  1 drivers
v0x13f714630_0 .net "pc_in", 31 0, v0x13f714940_0;  1 drivers
v0x13f7146e0_0 .var "pc_out", 31 0;
v0x13f7147a0_0 .net "reset", 0 0, v0x13f714ac0_0;  1 drivers
E_0x13f704520 .event posedge, v0x13f704580_0;
    .scope S_0x13f7042d0;
T_0 ;
    %wait E_0x13f704520;
    %load/vec4 v0x13f7147a0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0x13f7146e0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x13f714630_0;
    %assign/vec4 v0x13f7146e0_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x13f704160;
T_1 ;
    %vpi_call 2 17 "$dumpfile", "pc_test.vcd" {0 0 0};
    %vpi_call 2 18 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x13f704160 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x13f7148a0_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x13f704160;
T_2 ;
    %delay 5000, 0;
    %load/vec4 v0x13f7148a0_0;
    %inv;
    %store/vec4 v0x13f7148a0_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x13f704160;
T_3 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x13f714ac0_0, 0, 1;
    %pushi/vec4 10, 0, 32;
    %store/vec4 v0x13f714940_0, 0, 32;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x13f714ac0_0, 0, 1;
    %pushi/vec4 4, 0, 32;
    %store/vec4 v0x13f714940_0, 0, 32;
    %delay 10000, 0;
    %pushi/vec4 8, 0, 32;
    %store/vec4 v0x13f714940_0, 0, 32;
    %delay 10000, 0;
    %vpi_call 2 35 "$display", "Final PC Value: %h", v0x13f7149f0_0 {0 0 0};
    %vpi_call 2 36 "$finish" {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
